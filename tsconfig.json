{
	"extends": "@raythurnevoid/svelte-sapper-template/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"target": "ES2020",
		"types": [
			"svelte",
			"node",
			"sapper/runtime"
		],
		"importsNotUsedAsValues": "remove",
		"paths": {
			"@svelte-material-design/core/*": [
				"./node_modules/@svelte-material-ui-test/core/cjs/*",
				"./node_modules/@svelte-material-ui-test/core/ts/*",
				"./node_modules/@svelte-material-ui-test/core/packages/*"
			],
			"@svelte-material-design/space-modules/*": [
				"./node_modules/@svelte-material-design/space-modules/packages/*"
			],
			"svelte-prism/*": [
				"./node_modules/@raythurnevoid/svelte-prism/*"
			]
		}
	},
	"include": [
		"src",
	],
	"exclude": [
		"node_modules/**",
		"__sapper__/**",
		"static",
		"src/**/*.scss.d.ts"
	]
}